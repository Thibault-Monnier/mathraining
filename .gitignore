# Ignore bundler config
/.bundle

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore other unneeded files
*.tmp
*.swp

# Variables
/config/app_environment_variables.rb

# Assets
/public/assets/

# Files
/public/system/
/storage/

# Schema
/db/schema.rb

# SimpleCov
/coverage

# Ignore master key for decrypting credentials and more.
/config/master.key
