<%= form_for @submission, url: create_intest_problem_submissions_path(@problem), :html => { :multipart => true, :class => "mb-2" } do |f| %>
  <%= render 'submissions/form', f: f %>
  <%= render 'shared/edit_files' %>

  <%= f.submit "Enregistrer cette solution", class: "btn btn-primary mb-1", :disabled => in_skin? %>
  <button class="btn btn-ld-light-dark mb-1" onclick="return Rolling.hideActual();">Annuler</button>  
<% end %>

<p class="fst-italic">Vous pourrez modifier cette solution tant que vous le voudrez dans le temps imparti.</p>
