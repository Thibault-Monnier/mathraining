<% if @what == 0 %>
  <% str = "correct" %>
<% else %>
  <% str = "false" %>
<% end %>
var loading = document.getElementById("loading_<%= str %>");
var none = document.getElementById("none_<%= str %>");
var tab = document.getElementById("all_<%= str %>");
var tbody = tab.getElementsByTagName("tbody")[0];

<% @submissions.each do |s| %>
  tbody.insertAdjacentHTML('beforeend', '<%= escape_javascript(render 'submissions/showline', s: s) %>');
<% end %>

loading.style.display = 'none';

<% if @submissions.empty? %>
  none.style.display = 'inline';
<% else %>
  tab.style.display = 'table';
  tab.style.width = 'auto';
<% end %>
